{
    "name": "webpack-webfont",
    "version": "0.0.1-alpha.10",
    "description": "Webpack plugin for webfont package",
    "license": "MIT",
    "author": "itgalaxy <development@itgalaxy.company>",
    "contributors": [
        {
            "name": "Alexander Krasnoyarov",
            "email": "alexander.krasnoyarov@itgalaxy.company",
            "url": "https://vk.com/sterling_archer"
        }
    ],
    "repository": {
        "type": "https",
        "url": "https://github.com/itgalaxy/webpack-webfont"
    },
    "keywords": [
        "cli",
        "standalone",
        "font",
        "fonts",
        "webfont",
        "webfonts",
        "svg",
        "ttf",
        "woff",
        "woff2",
        "otf",
        "ttf",
        "woff",
        "eot",
        "svg",
        "ttf2eot",
        "ttf2woff",
        "ttf2svg",
        "svg2ttf",
        "css",
        "scss",
        "webpack"
    ],
    "bugs": {
        "url": "https://github.com/itgalaxy/webpack-webfont/issues"
    },
    "main": "dist/WebfontPlugin.js",
    "files": [
        "dist",
        "!**/__tests__"
    ],
    "dependencies": {
        "fs-extra": "^4.0.0",
        "glob": "^7.1.2",
        "glob-parent": "^3.1.0",
        "nodeify": "^1.0.0",
        "webfont": "^7.0.0"
    },
    "devDependencies": {
        "ava": "^0.22.0",
        "babel-cli": "^6.11.0",
        "babel-core": "^6.11.0",
        "babel-preset-es2015": "^6.9.0",
        "babel-preset-stage-0": "^6.5.0",
        "babel-register": "^6.9.0",
        "bluebird": "^3.4.0",
        "coveralls": "^2.11.6",
        "css-loader": "^0.28.0",
        "del": "^3.0.0",
        "del-cli": "^1.1.0",
        "eslint": "^4.1.1",
        "eslint-plugin-ava": "^4.2.1",
        "eslint-plugin-import": "^2.6.0",
        "eslint-plugin-itgalaxy": "^50.0.0",
        "eslint-plugin-jest": "^20.0.3",
        "eslint-plugin-jsx-a11y": "^6.0.0",
        "eslint-plugin-lodash": "^2.4.3",
        "eslint-plugin-node": "^5.1.0",
        "eslint-plugin-prettier": "^2.1.2",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-react": "^7.1.0",
        "eslint-plugin-unicorn": "^2.1.2",
        "file-loader": "^0.11.0",
        "sinon": "^4.0.0",
        "npmpub": "^3.1.0",
        "npm-run-all": "^4.0.0",
        "nyc": "^11.0.0",
        "remark-cli": "^4.0.0",
        "remark-preset-lint-itgalaxy": "^7.0.1",
        "style-loader": "^0.19.0",
        "webpack": "^4.0.0",
        "url-loader": "^0.6.1",
        "husky": "^0.14.0",
        "lint-staged": "^4.0.0",
        "prettier": "^1.5.2"
    },
    "scripts": {
        "precommit": "lint-staged",
        "lint:eslint": "eslint . --ignore-path .gitignore  --color",
        "lint:remark": "remark . -i .gitignore -f -q",
        "lint": "npm-run-all -l --parallel lint:*",
        "prettier": "eslint . --fix --ignore-path .gitignore",
        "pretest": "npm run lint",
        "ava": "nyc ava --verbose \"src/**/__tests__/*.js\"",
        "test": "npm run ava",
        "prebuild": "del dist",
        "build": "babel src --out-dir dist --ignore /__tests__/",
        "prepublish": "npm run build",
        "release": "npmpub"
    },
    "ava": {
        "require": "babel-register",
        "concurrency": 5,
        "babel": "inherit"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-0"
        ]
    },
    "eslintConfig": {
        "rules": {
            "node/no-unsupported-features": "off"
        },
        "extends": [
            "plugin:itgalaxy/ava",
            "plugin:itgalaxy/esnext",
            "plugin:itgalaxy/node"
        ],
        "root": true
    },
    "remarkConfig": {
        "plugins": [
            "remark-preset-lint-itgalaxy"
        ]
    }
}
